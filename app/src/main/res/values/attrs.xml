<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="customFontName" format="enum">
        <enum name="ClanOT_NarrBold" value="0"/>
        <enum name="ClanOT_NarrBook" value="1"/>
        <enum name="ClanOT_NarrThin" value="2"/>
        <enum name="ClanOT_News" value="3"/>
    </attr>
    <declare-styleable name="CustomFontTextView">
        <attr name="customFontName"/>
    </declare-styleable>

    <declare-styleable name="CustomToolbar">
        <attr name="withBackBtn" format="boolean"/>
        <attr name="titleText" format="string"/>
    </declare-styleable>
</resources>